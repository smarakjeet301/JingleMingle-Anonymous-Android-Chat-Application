<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jinglemingle.jinglemingle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/final_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/final_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="jinglemingle.jinglemingle.jinglemingle.MainActivity"/>
        <activity android:name="jinglemingle.jinglemingle.jinglemingle.WelcomeActivity"/>



        <activity android:name="jinglemingle.jinglemingle.jinglemingle.SplashScreen">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>